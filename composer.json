{
    "name": "minube/phalcon-autoload-services",
    "type": "library",
    "description": "Components for automatic registration services in Dependency Injection",
    "keywords": [ "framework", "phalcon", "di", "dependency injection" ],
    "license": "MIT",
    "require": {
        "php": ">=5.3.0",
        "ext-phalcon": ">=1.2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "4.5.*",
        "squizlabs/php_codesniffer": "2.*",
        "codeception/codeception": "*"
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "1.1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "GetSky\\Phalcon\\AutoloadServices\\": "src/"
        }
    }
}
